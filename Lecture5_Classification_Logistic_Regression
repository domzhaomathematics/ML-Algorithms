eturn x, mean_x, std_x\n","\n","\n","def de_standardize(x, mean_x, std_x):\n","    \"\"\"Reverse the procedure of standardization.\"\"\"\n","    x = x * std_x\n","    x = x + mean_x\n","    return x\n","\n","\n","def build_model_data(height, weight):\n","    \"\"\"Form (y,tX) to get regression data in matrix form.\"\"\"\n","    y = weight\n","    x = height\n","    num_samples = len(y)\n","    tx = np.c_[np.ones(num_samples), x]\n","    return y, tx\n","\n","def load_data(data,sub_sample=True, add_outlier=False):\n","    height = data.iloc[:, 1]\n","    weight = data.iloc[:, 2]\n","    gender=data.iloc[:,0]\n","    # Convert to metric system\n","    height *= 0.025\n","    weight *= 0.454\n","    return height, weight, gender\n","def sample_data(y, x, seed, size_samples):\n","    \"\"\"sample from dataset.\"\"\"\n","    np.random.seed(seed)\n","    num_observations = y.shape[0]\n","    random_permuted_indices = np.random.permutation(num_observations)\n","    y = y[random_permuted_indices]\n","    x = x[random_permuted_indices]\n","    return y[:size_samples], x[:size_samples]"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"ftTT-4czANTj","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":119},"outputId":"e6ae56a1-6995-4273-9105-bf1054cad6c8","executionInfo":{"status":"ok","timestamp":1564487462875,"user_tz":-120,"elapsed":635,"user":{"displayName":"Dominic Zhao","photoUrl":"","userId":"13142109890950216893"}}},"source":["height,weight,gender=load_data(data)\n","new_array=[]\n","for row in gender:\n","  if row==\"Male\":\n","    new_array.append(0)\n","  else:\n","    new_array.append(1)\n","gender=pd.Series(new_array)\n","height=np.array(height)\n","weight=np.array(weight)\n","print(gender.head())"],"execution_count":5,"outputs":[{"output_type":"stream","text":["0    0\n","1    0\n","2    0\n","3    0\n","4    0\n","dtype: int64\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"K57kvJPCBsbJ","colab_type":"code","colab":{}},"source":["def least_squares(y,tx):\n","  #this function will give the optimal solution\n","  a=tx.T.dot(tx)\n","  b=tx.T.dot(y)\n","\n","  return np.linalg.solve(a,b)"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"lOW7dHv8DOsN","colab_type":"text"},"source":["**Plots**"]},{"cell_type":"code","metadata":{"id":"9lARMhM4DRN0","colab_type":"code","colab":{}},"source":["\n","def visualization(y, x, mean_x, std_x, w, save_name):\n","    \"\"\"visualize the raw data as well as the classification result.\"\"\"\n","    fig = plt.figure()\n","    # plot raw data\n","    x = de_standardize(x, mean_x, std_x)\n","    ax1 = fig.add_subplot(1, 2, 1)\n","    males = np.where(y == 1)\n","    females = np.where(y == 0)\n","    ax1.scatter(\n","        x[males, 0], x[males, 1],\n","        marker='.', color=[0.06, 0.06, 1], s=20)\n","    ax1.scatter(\n","        x[females, 0], x[females, 1],\n","        marker='*', color=[1, 0.06, 0.06], s=20)\n","    ax1.set_xlabel(\"Height\")\n","    ax1.set_ylabel(\"Weight\")\n","    ax1.grid()\n","    # plot raw data with decision boundary\n","    ax2 = fig.add_subplot(1, 2, 2)\n","    height = np.arange(\n","        np.min(x[:, 0]), np.max(x[:, 0]) + 0.01, step=0.01)\n","    weight = np.arange(\n","        np.min(x[:, 1]), np.max(x[:, 1]) + 1, step=1)\n","    hx, hy = np.meshgrid(height, weight)\n","    hxy = (np.c_[hx.reshape(-1), hy.reshape(-1)] - mean_x) / std_x\n","    x_temp = np.c_[np.ones((hxy.shape[0], 1)), hxy]\n","    prediction = x_temp.dot(w) > 0.5\n","    prediction = prediction.reshape((weight.shape[0], height.shape[0]))\n","    ax2.contourf(hx, hy, prediction, 1)\n","    ax2.scatter(\n","        x[males, 0], x[males, 1],\n","        marker='.', color=[0.06, 0.06, 1], s=20)\n","    ax2.scatter(\n","        x[females, 0], x[females, 1],\n","        marker='*', color=[1, 0.06, 0.06], s=20)\n","    ax2.set_xlabel(\"Height\")\n","    ax2.set_ylabel(\"Weight\")\n","    ax2.set_xlim([min(x[:, 0]), max(x[:, 0])])\n","    ax2.set_ylim([min(x[:, 1]), max(x[:, 1])])\n","    plt.tight_layout()\n","    plt.savefig(save_name)"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"291q1cyYCcc7","colab_type":"text"},"source":["**Classification using linear regression**"]},{"cell_type":"code","metadata":{"id":"M823zOf-F1zl","colab_type":"code","colab":{}},"source":["seed=1\n","y = np.expand_dims(gender, axis=1)\n","X = np.c_[height.reshape(-1), weight.reshape(-1)]\n","y, X = sample_data(y, X, seed, size_samples=200)\n","x, mean_x, std_x = standardize(X)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"ACf3Q5qYCRqy","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":297},"outputId":"d7749542-abbb-4a2e-bf32-0952ec3d7921","executionInfo":{"status":"ok","timestamp":1564487529340,"user_tz":-120,"elapsed":1176,"user":{"displayName":"Dominic Zhao","photoUrl":"","userId":"13142109890950216893"}}},"source":["def least_square_classification_demo(y, x):\n","    # classify the data by linear regression\n","    tx = np.c_[np.ones((y.shape[0], 1)), x]\n","    w = least_squares(y, tx)\n","\n","    # visualize your classification.\n","    visualization(y, x, mean_x, std_x, w, \"classification_by_least_square\")\n","    \n","least_square_classification_demo(y, x)"],"execution_count":13,"outputs":[{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAAagAAAEYCAYAAAAJeGK1AAAABHNCSVQICAgIfAhkiAAAAAlwSFlz\nAAALEgAACxIB0t1+/AAAADl0RVh0U29mdHdhcmUAbWF0cGxvdGxpYiB2ZXJzaW9uIDMuMC4zLCBo\ndHRwOi8vbWF0cGxvdGxpYi5vcmcvnQurowAAIABJREFUeJztvXt4FeW59/+5k5CEQyDhFNCKoBA5\n6PaUgqfWoK09qdRiReqp1bfW1lZ3BdS2v1Z3td1YtVbbX7tFa9W24nG7FftWtO7GVihaEKuAgCiI\nETUQSEIISchaz/vHrMmaNZlZp6zzuj/XlWutmTUz6xlguHM/z/f+3mKMQVEURVFyjZJsD0BRFEVR\nvNAApSiKouQkGqAURVGUnEQDlKIoipKTaIBSFEVRchINUIqiKEpOogFKURRFyUk0QCmKoig5iQYo\nRVEUJScpy/YABsLo0aPNxIkT2bdvH0OHDs32cDJGMd1vrt3rmjVrdhljxmR7HKlmUMVQUzFkZLaH\noeQJY7vbGdmzr297d/lQmiuGx